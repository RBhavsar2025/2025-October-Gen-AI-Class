   "source": [
    "filename = input (\"Enter the filename\")\n",
    "if filename.endswith(\".csv\"):\n",
    "    print (\"File type is CSV\")\n",
    "    \n",
    "elif filename.endswith(\".doc\"):\n",
    "    print (\"File type is DOC\")\n",
    "\n",
    "else:\n",
    "    print (\"File type is not supported\")"
   ]
  }
